{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-inferrable-types": 0,
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ]
  }
}
